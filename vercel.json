{
  "version": 2,
  "builds": [
    {
      "src": "api/topsis.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/topsis",
      "dest": "/api/topsis.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}